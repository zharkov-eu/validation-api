{
  "name": "validation-api",
  "version": "2.0.0",
  "description": "TypeScript Validation Api",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register **/*.spec.ts",
    "build": "npm run clean && rollup -c",
    "clean": "rimraf dist",
    "format": "prettier --write **/*.{ts,js,jsx,tsx,json,css,md}",
    "preversion": "npm run build && npm run test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zharkov-eu/validation-api.git"
  },
  "keywords": [
    "validation"
  ],
  "author": "Evgenii Zharkov <zharkov.ev.u@yandex.ru>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zharkov-eu/validation-api/issues"
  },
  "homepage": "https://github.com/zharkov-eu/validation-api#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "4.1.1",
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "chai": "4.2.0",
    "mocha": "7.1.2",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "rollup": "2.7.3",
    "rollup-plugin-dts": "1.4.0",
    "ts-node": "8.9.1",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  },
  "dependencies": {}
